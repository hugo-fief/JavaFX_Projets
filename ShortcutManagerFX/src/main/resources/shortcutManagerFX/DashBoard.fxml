<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shortcutManagerFX.controller.DashBoardController" spacing="15" style="-fx-padding: 20;">
    <Button fx:id="saveButton" text="Save" onAction="#performSaveAction" />
    <Button fx:id="openButton" text="Open" onAction="#performOpenAction" />
    <Button text="Configure Shortcuts" onAction="#openConfigWindow" />
</VBox>
